/cache              ext4        /dev/block/by-name/cache     flags=display="Cache";wipeingui;wipeduringfactoryreset;backup=0
/data               ext4        /dev/block/by-name/data      flags=display="Data";wipeingui;wipeduringfactoryreset;backup=1
/system             ext4        /dev/block/by-name/system    flags=display="System";backup=1;wipeingui
/boot               emmc        /dev/block/by-name/boot      flags=display="Boot";backup=1
/recovery           emmc        /dev/block/by-name/recovery  flags=display="Recovery";backup=1
#/fastboot          emmc        /dev/block/by-name/fastboot
/external_sd	    vfat	    /dev/block/mmcblk1p1	/dev/block/mmcblk1    flags=display="External SDcard";settingsstorage;wipeingui;removable;backup=0
/sd-ext             auto        /dev/block/mmcblk1p2
/usb-otg	        vfat		/dev/block/sda1		/dev/block/sda		flags=display="USB-OTG";storage;wipeingui;removable
